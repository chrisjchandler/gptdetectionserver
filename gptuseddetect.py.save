import openai
import os

# Set the OPENAI_API_KEY environment variable
os.environ["OPENAI_API_KEY"] = "your_api_key_goes_here"

# Use the OpenAI API to create a new language model
model = openai.Model.create(engine="davinci")

# Ask the user to enter the text to classify
text = input("Enter the text to classify: ")

# Define the prompt for the language model
prompt = (
    f"Please classify the following text as being generated by OpenAI or not: \n"
    f"text: {text}"
)

# Generate a response from the language model
response = model.completion(prompt=prompt, temperature=0.5, max_tokens=1024)

# Print the response
print(response)
